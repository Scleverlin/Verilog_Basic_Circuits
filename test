v0x55b74085fe30_0 .var "rst", 0 0;
v0x55b74085ff00_0 .net "sum", 7 0, v0x55b74085f5d0_0;  1 drivers
S_0x55b740808ff0 .scope module, "m1" "pipeline_8bit_adder" 2 8, 3 1 0, S_0x55b740808dc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "enable";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 8 "a";
    .port_info 3 /INPUT 8 "b";
    .port_info 4 /INPUT 1 "cin";
    .port_info 5 /OUTPUT 1 "cout";
    .port_info 6 /OUTPUT 8 "sum";
v0x55b74080aba0_0 .net "a", 7 0, v0x55b74085fa10_0;  1 drivers
v0x55b7408306c0_0 .net "b", 7 0, v0x55b74085faf0_0;  1 drivers
v0x55b74085f0e0_0 .net "cin", 0 0, v0x55b74085fbc0_0;  1 drivers
v0x55b74085f180_0 .var "cout", 0 0;
v0x55b74085f240_0 .net "enable", 0 0, v0x55b74085fd90_0;  1 drivers
v0x55b74085f350_0 .var "high_bit_sum", 3 0;
v0x55b74085f430_0 .var "low_bit_sum", 3 0;
v0x55b74085f510_0 .net "rst", 0 0, v0x55b74085fe30_0;  1 drivers
v0x55b74085f5d0_0 .var "sum", 7 0;
v0x55b74085f6b0_0 .var "temp_a", 3 0;
v0x55b74085f790_0 .var "temp_b", 3 0;
v0x55b74085f870_0 .var "temp_cin", 0 0;
E_0x55b740848480/0 .event negedge, v0x55b74085f510_0;
E_0x55b740848480/1 .event posedge, v0x55b74085f240_0;
E_0x55b740848480 .event/or E_0x55b740848480/0, E_0x55b740848480/1;
    .scope S_0x55b740808ff0;
T_0 ;
    %wait E_0x55b740848480;
    %load/vec4 v0x55b74085f510_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55b74085f5d0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b74085f180_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55b74080aba0_0;
    %parti/s 4, 0, 2;
    %pad/u 5;
    %load/vec4 v0x55b7408306c0_0;
    %parti/s 4, 0, 2;
    %pad/u 5;
    %add;
    %load/vec4 v0x55b74085f0e0_0;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %assign/vec4 v0x55b74085f430_0, 0;
    %assign/vec4 v0x55b74085f870_0, 0;
    %load/vec4 v0x55b74080aba0_0;
    %parti/s 4, 4, 4;
    %assign/vec4 v0x55b74085f6b0_0, 0;
    %load/vec4 v0x55b7408306c0_0;
    %parti/s 4, 4, 4;
    %assign/vec4 v0x55b74085f790_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55b740808ff0;
T_1 ;
    %wait E_0x55b740848480;
    %load/vec4 v0x55b74085f510_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55b74085f5d0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b74085f180_0, 0, 1;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55b74085f6b0_0;
    %pad/u 5;
    %load/vec4 v0x55b74085f790_0;
    %pad/u 5;
    %add;
    %load/vec4 v0x55b74085f870_0;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %ix/load 4, 4, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55b74085f5d0_0, 4, 5;
    %assign/vec4 v0x55b74085f180_0, 0;
    %load/vec4 v0x55b74085f6b0_0;
    %pad/u 5;
    %load/vec4 v0x55b74085f790_0;
    %pad/u 5;
    %add;
    %load/vec4 v0x55b74085f870_0;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %assign/vec4 v0x55b74085f350_0, 0;
    %assign/vec4 v0x55b74085f180_0, 0;
    %load/vec4 v0x55b74085f430_0;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x55b74085f5d0_0, 4, 5;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55b740808dc0;
T_2 ;
    %vpi_call 2 12 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55b740808dc0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b74085fd90_0, 0, 1;
T_3.0 ;
    %delay 10, 0;
    %load/vec4 v0x55b74085fd90_0;
    %inv;
    %store/vec4 v0x55b74085fd90_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x55b740808dc0;
T_4 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55b74085fa10_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55b74085faf0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b74085fbc0_0, 0, 1;
