:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b740808dc0 .scope module, "tb_8_bitadder" "tb_8_bitadder" 2 1;
 .timescale 0 0;
P_0x55b740808f50 .param/l "delay" 0 2 7, +C4<00000000000000000000000001010000>;
v0x55b74085fa10_0 .var "a", 7 0;
v0x55b74085faf0_0 .var "b", 7 0;
v0x55b74085fbc0_0 .var "cin", 0 0;
v0x55b74085fcc0_0 .net "cout", 0 0, v0x55b74085f180_0;  1 drivers
